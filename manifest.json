{
  "manifest_version": 2,

  "name": "Web page counter",
  "description": "This extension counts a number of visited/opened web pages.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Веб лічильник"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "http://*/*",
    "https://*/*"
  ]
}